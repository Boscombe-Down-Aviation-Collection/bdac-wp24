{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "color": {
            "defaultPalette": false,
            "palette": [
                {
                    "slug": "dev23-red",
                    "color": "#d91f26",
                    "name": "Dev Red"
                },
                {
                    "slug": "dev23-blue",
                    "color": "#071f5b",
                    "name": "Dev Blue"
                },
                {
                    "slug": "dev23-blue-alt",
                    "color": "#dbe2f3",
                    "name": "Dev Blue Alt"
                },
                {
                    "slug": "dev23-yellow",
                    "color": "#fcba04",
                    "name": "Dev yellow"
                },
                {
                    "slug": "dev23-white",
                    "color": "#f7f7ff",
                    "name": "Dev White"
                },
                {
                    "slug": "dev23-black",
                    "color": "#304048",
                    "name": "Dev Black"
                }
            ],
            "background": true,
            "text": true,
            "link": true,
            "custom": true,
            "duotone": [
                {
                    "slug": "dev-bdac-sunset",
                    "colors": [
                        "#071f5b",
                        "#d91f26"
                    ],
                    "name": "Dev BDAC Sunset"
                }
            ],
            "defaultGradients": false,
            "gradients": [
                {
                    "slug": "dev-summer-dog",
                    "name": "Dev Summer Dog",
                    "gradient": "linear-gradient(90deg, #d91f26, #071f5b)"
                }
            ]
        },
        "blocks": {
            "core/site-title": {
                "color": {
                    "defaultPalette": false,
                    "custom": false,
                    "gradients": [],
                    "customGradient": false
                }
            },
            "core/cover": {
                "color": {
                    "duotone": []
                }
            }
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--dev-23-white)",
            "text": "var(--wp--preset--color--dev-23-black)"
        },
        "blocks": {
            "core/button": {
                "color": {
                    "background": "var(--wp--preset--color--dev-23-red)",
                    "text": "var(--wp--preset--color--dev-23-white)"
                },
                "border": {
                    "color": "var(--wp--preset--color--dev-23-red)",
                    "style": "solid",
                    "radius": "0.625rem",
                    "width": "0.125rem"
                }      
            }
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--dev-23-red)"
                }
            },
            "h1": {
                "color": {
                    "text": "var(--wp--preset--color--dev-23-blue)"
                }
            },
            "h2": {
                "color": {
                    "text": "var(--wp--preset--color--dev-23-blue)"
                }
            },
            "h3": {
                "color": {
                    "text": "var(--wp--preset--color--dev-23-blue)"
                }
            },
            "h4": {
                "color": {
                    "text": "var(--wp--preset--color--dev-23-blue)"
                }
            },
            "h5": {
                "color": {
                    "text": "var(--wp--preset--color--dev-23-blue)"
                }
            },
            "h6": {
                "color": {
                    "text": "var(--wp--preset--color--dev-23-blue)"
                }
            }
        }
    }
}
